<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.performance.mapper.TestMapper">

    <select id="selectAllProjectCodes" resultType="java.lang.String">
        SELECT
            DISTINCT proj_code
        FROM
            zwjg_data_center.dc_project_v1
        ORDER BY proj_code
    </select>
    
</mapper>